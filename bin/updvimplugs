#!/bin/sh
echo "Fetch updates from vim plugins"
plugged="$HOME/.config/nvim/plugged" 
if [ -d $plugged ]
then
  cd $plugged
  for d in $plugged/*
  do
    if [ -d "$d/.git" ]
    then
      cd $d
      git pull
    fi
  done
else
  echo "No nvim config folder existent"
fi
echo "Vim plugins updated"
